    "container": {
        "image": "louislam/uptime-kuma:1-alpine",
        "shell": "sh",
        "privileged": false,
        "network_model": "bridge",
        "web_ui": {
            "http": "3001",
            "path": "/"
        },
        "health_check": "",
        "envs": [],
        "ports": [
            {
                "container": "3001",
                "host": "3001",
                "type": "tcp",
                "allocation": "preferred",
                "configurable": "advanced",
                "description": "WebUI Port"
            }
        ],
        "volumes": [
            {
                "container": "/app/data",
                "host": "/DATA/AppData/$AppID/app/data",
                "mode": "rw",
                "allocation": "automatic",
                "configurable": "advanced",
                "description": "Uptime Kuma data directory."
            }
        ],
        "devices": [],
        "constraints": {
            "min_memory": 128,
            "min_storage": 128
        },
        "restart_policy": "unless-stopped",
        "sysctls": [],
        "cap_add": [],
        "labels": []
    },
    "abilities": {
        "notification": false,
        "widgets": false,
        "authentication": false,
        "search": false,
        "upnp": false
    },
    "tips": {
        "before_install": []
    },
