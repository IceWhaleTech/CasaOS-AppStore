    "container": {
        "image": "arcadiatechnology/crafty-4",
        "shell": "bash",
        "privileged": false,
        "network_model": "bridge",
        "web_ui": {
            "http": "8110",
            "path": "/panel"
        },
        "envs": [
            {
                "key": "TZ",
                "value": "Etc/UTC",
                "configurable": "yes",
                "description": "Timezone"
            }
        ],
        "ports": [
            {
