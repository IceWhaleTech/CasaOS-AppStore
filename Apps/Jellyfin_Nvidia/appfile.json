        "health_check": "",
        "envs": [],
        "ports": [
            {
                "container": "8096",
                "host": "8097",
                "type": "tcp",
                "allocation": "preferred",
                "configurable": "advanced",
                "description": "WebUI HTTP Port"
            },
            {
                "container": "8920",
                "host": "8921",
                "type": "tcp",
                "allocation": "preferred",
