    "container": {
        "image": "linuxserver/pyload-ng:0.5.0",
        "shell": "bash",
        "privileged": false,
        "network_model": "bridge",
        "web_ui": {
            "http": "",
            "path": ""
        },
        "health_check": "",
        "envs": [],
        "ports": [
            {
                "container": "8000",
                "host": "8000",
                "type": "tcp",
                "allocation": "preferred",
                "configurable": "advanced",
                "description": "WebUI HTTP Port"
            },
            {
                "container": "9666",
                "host": "9666",
                "type": "tcp",
                "allocation": "preferred",
                "configurable": "advanced",
                "description": "PyLoad listening Port (TCP)"
            }
        ],
        "volumes": [
            {
                "container": "/config",
                "host": "/DATA/AppData/$AppID/config",
                "mode": "rw",
                "allocation": "automatic",
                "configurable": "no",
                "description": "PyLoad config directory."
            },
            {
                "container": "/downloads",
                "host": "/DATA/Downloads",
                "mode": "rw",
                "allocation": "automatic",
                "configurable": "no",
                "description": "PyLoad Download Directory."
            }
        ],
        "devices": [],
        "constraints": {
            "min_memory": 0,
            "min_storage": 0
        },
