    "container": {
        "image": "privoce/vocechat-server:v0.3.33",
        "shell": "bash",
        "privileged": false,
        "network_model": "bridge",
        "web_ui": {
            "http": "3000",
            "path": "/"
        },
        "health_check": "",
        "envs": [
            {
                "key": "TZ",
                "value": "$TZ",
                "configurable": "yes",
                "description": "TimeZone"
            }
        ],
        "ports": [
            {
                "container": "3000",
                "host": "3009",
                "type": "tcp",
                "allocation": "preferred",
                "configurable": "advanced",
                "description": "WebUI HTTP Port"
            }
        ],
        "volumes": [
            {
                "container": "/home/vocechat-server/data",
                "host": "/DATA/AppData/$AppID/home/vocechat-server/data",
                "mode": "rw",
                "allocation": "automatic",
                "configurable": "no",
                "description": "VoceChat Data"
            }
        ],
        "devices": [],
        "constraints": {
            "min_memory": 64,
            "min_storage": 1024
        },
        "restart_policy": "unless-stopped",
        "sysctls": [],
        "cap_add": [],
        "labels": [],
        "host_name": "",
        "cmd": []
    },
