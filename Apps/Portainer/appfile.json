    "container": {
        "image": "portainer/portainer-ce:latest",
        "shell": "bash",
        "privileged": true,
        "network_model": "bridge",
        "web_ui": {
            "https": "9443",
            "path": "/"
        },
        "health_check": "",
        "envs": [],
        "ports": [
            {
                "container": "8000",
                "host": "8000",
                "type": "tcp",
                "allocation": "preferred",
                "configurable": "advanced",
                "description": "TCP tunnel server"
            },
            {
                "container": "9443",
                "host": "9443",
                "type": "tcp",
                "allocation": "preferred",
                "configurable": "advanced",
                "description": "WebUI HTTPS Port"
            }
        ],
        "volumes": [
            {
                "container": "/var/run/docker.sock",
                "host": "/var/run/docker.sock",
                "mode": "rw",
                "allocation": "automatic",
                "configurable": "no",
                "description": "Jellyfin config directory."
            },
            {
                "container": "/data",
                "host": "/DATA/AppData/$AppID/data",
                "mode": "rw",
                "allocation": "automatic",
                "configurable": "advanced",
                "description": "Jellyfin Media directory."
            }
        ],
        "devices": [],
        "constraints": {
            "min_memory": 32,
            "min_storage": 512
        },
        "restart_policy": "unless-stopped",
        "sysctls": [],
        "cap_add": {},
        "labels": []
    },
    "abilities": {
        "notification": false,
        "widgets": false,
        "authentication": true,
        "search": false,
        "upnp": false
